#!/bin/bash
case $REMOTE_HOSTNAME in
    mlaptop)
        echo "1440x900"
        ;;
    house_pc)
        echo "1280x1024"
        ;;
    maths_laptop)
        echo "1280x800"
        ;;
    house_laptop)
        echo "1366x768"
        ;;
    home_pc)
        echo "1680x1050"
        ;;
    small)
        echo "650x650"
        ;;
    art_laptop)
        echo "1536x864"
        ;;
    library_pc)
        echo "1024x768"
        ;;
    claptop)
        echo "1280x800"
        ;;
esac
